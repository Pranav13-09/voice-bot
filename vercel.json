{
    "version": 2,
    "name": "project_deployment",
    "builds": [{"src": "server.js", "use": "@vercel/node"}],
 "rewrites": [
    { "source": "/socket.io/(.*)", "destination": "/socket.io/$1" },
    { "source": "/(.*)", "destination": "/server.js" }
  ],
  "routes": [
    { "src": "/socket.io/(.*)", "dest": "/socket.io/$1" }
  ]
}


